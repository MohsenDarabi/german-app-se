{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cp -r content apps/web/content && cd apps/web && npx pnpm@10.18.2 run build",
  "installCommand": "npx pnpm@10.18.2 install",
  "outputDirectory": "apps/web/build",
  "framework": null,
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
